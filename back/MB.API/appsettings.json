{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "ConnectionStrings": {
    "DB": "Data Source = your_server ; Initial Catalog = your_database ; Integrated Security = True ; Trusted_Connection = True ;"
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "your_custom_jwt_key",
    "Issuer": "your_issuer",
    "Audience": "your_audience"
  }
}